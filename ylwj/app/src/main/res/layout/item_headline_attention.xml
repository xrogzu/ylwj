            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/margin_icon_left"
            android:layout_alignBottom="@+id/iv_headline_item12"
            android:layout_alignLeft="@id/tv_headline_item1_name2"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="长江七号，宇宙飞船等两人新关注了你" />

        <TextView
            android:id="@+id/tv_headline_item1_time2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/tv_headline_item1_name2"
            android:layout_marginRight="@dimen/margin_icon_left"
            android:text="11:15" />
    </RelativeLayout>

